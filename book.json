{
  "gitbook": "3.2.x",
  "title": "ceb: custom-element-builder",
  "root": "./doc",
  "plugins": [
    "prism",
    "-highlight",
    "github",
    "ga",
    "sitemap"
  ],
  "pluginsConfig": {
