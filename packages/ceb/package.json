{
  "name": "@tmorin/ceb",
  "version": "4.0.0-alpha.4",
  "license": "MIT",
  "description": "The package is part of the `<ceb/>` library. It bundles the main features of the library related to the Custom Elements (v1) specification.",
  "keywords": [
    "custom-element-builder",
    "custom-elements-v1",
    "custom-elements",
    "custom-element",
    "typescript",
    "typescript-library",
    "javascript-library",
    "template",
    "template-engine",
    "template-literal"
  ],
  "homepage": "https://tmorin.github.io/ceb",
  "bugs": {
    "url": "https://github.com/tmorin/ceb/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:tmorin/ceb.git"
  },
  "author": {
    "name": "Thibault Morin",
    "url": "https://tmorin.github.io"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "commonjs",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "unpkg": "umd/ceb.min.js",
  "files": [
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run build:umd",
    "build:lib": "tsc -p tsconfig.build.json --module CommonJS --outDir lib",
    "build:umd": "webpack --config webpack.prd.js --config webpack.dev.js"
  },
  "dependencies": {
    "@tmorin/ceb-builders": "^4.0.0-alpha.4",
    "@tmorin/ceb-core": "^4.0.0-alpha.4",
    "@tmorin/ceb-templating-builder": "^4.0.0-alpha.4",
    "@tmorin/ceb-templating-literal": "^4.0.0-alpha.4"
  }
}
